}

Abstract Class Media {
#string titre
#int dureeEmprunt
}
Class Magazine {
-int numero
-dateTime datePublication
}


Class Emprunt {
-dateTime dateEmprunt
-dateTime dateretourEstime
-dateTime dateretourEffectif
}

Class Adherant {
-string numeroAdherant
-string nom
-string prenom
-string email
-dateTime dateAdhesion
}

